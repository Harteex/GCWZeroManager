<Window x:Class="GCWZeroManager2.TransferProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TransferProgressWindow" Height="239" Width="461" ResizeMode="NoResize" Topmost="True" ShowInTaskbar="False" Closed="Window_Closed" Closing="Window_Closing">
