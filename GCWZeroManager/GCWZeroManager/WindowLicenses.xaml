<Window x:Class="GCWZeroManager2.WindowLicenses"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Licenses" Height="400" Width="450">
    <Grid>
        <ComboBox Height="23" HorizontalAlignment="Stretch" Margin="5,12,5,0" Name="comboBox1" VerticalAlignment="Top" Width="Auto" ItemsSource="{Binding}" SelectedIndex="0" SelectionChanged="comboBox1_SelectionChanged">
            <ComboBoxItem Content="Code" />
            <ComboBoxItem Content="Icon: Install Software, Manage Software, System Upgrade" />
            <ComboBoxItem Content="Icon: About" />
        </ComboBox>
        <TextBox Height="Auto" HorizontalAlignment="Stretch" Margin="5,38,5,5" Name="textBox1" VerticalAlignment="Stretch" Width="Auto" TextWrapping="WrapWithOverflow" VerticalScrollBarVisibility="Auto" IsReadOnly="True" />
    </Grid>
</Window>
